                @Html.DisplayFor(modelItem => item.id_ensamble)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.id_gama)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id_telefono }) |
